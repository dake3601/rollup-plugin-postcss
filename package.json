{
  "name": "rollup-plugin-postcss",
  "version": "3.0.0",
  "description": "Seamless integration between Rollup and PostCSS",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "types/index.d.ts"
  ],
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js ",
    "build": "rollup -c",
    "clean": "rimraf dist",
    "lint": "eslint src --ext .js",
    "format": "prettier --write \"src/**/*.js\" \"*.{js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.js\" \"*.{js,json,md}\"",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/egoist/rollup-plugin-postcss.git"
  },
  "keywords": [
    "postcss",
    "rollup"
  ],
  "author": "EGOIST <0x142857@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/egoist/rollup-plugin-postcss/issues"
  },
  "types": "./types/index.d.ts",
  "homepage": "https://github.com/egoist/rollup-plugin-postcss#readme",
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^28.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.0",
    "fs-extra": "^11.2.0",
    "globals": "^15.0.0",
    "jest": "^29.7.0",
    "less": "^4.2.0",
    "postcss": "^8.4.0",
    "prettier": "^3.2.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.0",
    "sass": "^1.70.0",
    "stylus": "^0.62.0",
    "sugarss": "^4.0.1"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.0.0",
    "concat-with-sourcemaps": "^1.1.0",
    "cssnano": "^6.0.0",
    "p-queue": "^8.0.1",
    "pify": "^6.0.0",
    "postcss-load-config": "^5.0.0",
    "postcss-modules": "^6.0.0",
    "resolve": "^1.22.0",
    "style-inject": "^0.3.0"
  },
  "peerDependencies": {
    "postcss": "^8.0.0"
  }
}
